{
  "name": "@tabuckner/material-dayjs-adapter",
  "version": "3.0.0",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy": "../../node_modules/.bin/cpy schematics/**/{collection.json,schema.json,files/**} ../../dist/material-dayjs-adapter --parents",
    "postbuild": "npm run copy",
    "clean:dist": "rm -rf ../../dist/material-dayjs-adapter"
  },
  "author": {
    "name": "Taylor Buckner",
    "url": "https://github.com/tabuckner/"
  },
  "contributors": [
    {
      "name": "Jan van Rossum",
      "url": "https://github.com/vanrossumict"
    }
  ],
  "description": "A MatDateAdapter for Dayjs to reduce dependency sizes when compared to MomentJS based adapters.",
  "keywords": [
    "angular",
    "angular-material",
    "material",
    "datepicker",
    "mat-datepicker",
    "momentjs",
    "dayjs",
    "adapter",
    "mat-date-adapter",
    "date",
    "iso",
    "utc",
    "typescript"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": ">=13.0.0",
    "@angular/core": ">=13.0.0",
    "@angular/material": ">=13.0.0",
    "dayjs": ">=1.8.17"
  },
  "repository": {
    "url": "https://github.com/tabuckner/material-dayjs-adapter"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {}
}
